---
layout: feed
---


<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
 <title>Distiller's Blog</title>
 <link href="http://DataDistiller.github.io/quotes/atom.xml" rel="self"/>
 <link href="http://DataDistiller.github.io/quotes"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://DataDistiller.github.io/quotes</id>
 <author>
   <name>Data Distiller</name>
   <email>ashvin.chougule@gmail.com</email>
 </author>
 {% for saying in site.sayings %}
 <entry>
   <title>{{ saying.title }}</title>
   <link href="http://DataDistiller.github.io{{ saying.url }}"/>
   <updated>{{ saying.date | date_to_xmlschema }}</updated>
   <id>http://DataDistiller.github.io/{{ saying.id }}</id>
   <content type="html">{{ saying.content | xml_escape }}</content>
 </entry>
 {% endfor %}
 
 
</feed>

